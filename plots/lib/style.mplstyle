font.family : monospace
font.serif: Nimbus Roman
font.sans-serif: Nimbus Sans
font.cursive: Z003
font.fantasy: Z003
font.monospace: Nimbus Mono
# font.monospace: Nimbus Mono PS
# font.monospace: FreeMono

mathtext.fontset: custom
# mathtext.bf: sans:bold
# mathtext.bfit: sans:italic:bold
# mathtext.cal: cursive
# mathtext.it: sans:italic
# mathtext.rm: sans
# mathtext.sf: sans
# mathtext.tt: monospace
mathtext.fallback: None
mathtext.default: regular

text.usetex: False


# text.usetex: True
# text.latex.preamble: "\renewcommand*\familydefault{\ttdefault} \usepackage[noendash,LGRgreek]{mathastext} \MTfamily{\ttdefault} \MTgreekfont{\ttdefault} \Mathastext"

axes.formatter.useoffset: False
# axes.prop_cycle: cycler(color=['black'])
# axes.prop_cycle: cycler(color=['black']) * cycler(linestyle=['solid', 'dashed', 'dashdot', 'dotted'])
axes.titlesize : 10  # 12 pts == 1 pica
axes.labelsize : 10  # 10 pts

xtick.bottom: True
xtick.top: True
xtick.minor.visible: True
xtick.direction: in
xtick.labelsize : 10

ytick.left: True
ytick.right: True
ytick.minor.visible: True
ytick.direction: in
ytick.labelsize : 10

lines.color: k
lines.linestyle  : -
lines.linewidth: 0.8
lines.markersize: 1

patch.linewidth: 0.8

scatter.marker: .

legend.fontsize : 10
legend.framealpha: 1
legend.edgecolor: 0
legend.fancybox: False

figure.titlesize : 10
figure.labelsize : 10

# image.aspect : equal
image.cmap : binary
image.interpolation: none
image.resample: False

